<% include ./partials/header %>

<h1>Administrator's Dashboard</h1>

<h2>Users</h2>
<table>
  <tr>
    <td><b>Username</b></td>
    <td><b>ID</b></td>
  </tr>

<% users.forEach(function(user){ %>
  <tr>
    <td><%= user.username %></td>
    <td><%= user.id %></td>
  </tr>
<% }) %>

</table>
